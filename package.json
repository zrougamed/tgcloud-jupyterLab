{
  "name": "@jupyterlab/tgcloud",
  "version": "0.6.0",
  "description": "TigerGraph tgcloud jupyterLab/SageMaker Integration",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/zrougamed/tgcloud-jupyterLab",
  "bugs": {
    "url": "https://github.com/zrougamed/tgcloud-jupyterLab"
  },
  "license": "BSD-3-Clause",
  "author": "",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/zrougamed/tgcloud-jupyterLab.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "link": "jupyter labextension link . --no-build",
    "tslint": "tslint --fix -c tslint.json --project tsconfig.json \"**/*{.ts,.tsx}\"",
    "tslint:check": "tslint -c tslint.json --project tsconfig.json \"**/*{.ts,.tsx}\"",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/launcher": "^1.0.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "~3.5.2"
  },
  "sideEffects": [
    "style/*.css"
  ],
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "tgcloud-jupyter"
        }
      }
    }
  },
  "directories": {
    "lib": "lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true
  }
}